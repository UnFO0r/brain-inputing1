//输出一个正整数区间之间的质数（2和3除外）
//试除法
int main()
{
	int i,k;
	int count = 0;
	printf("please input: ");
	scanf("%d %d", &k,&i);//区间上下界
	int m = 0;//循环体变量
	for (m = k; m <= i; m++)
	{
		int j = 0;//除数
		for (j = 2; m % 2 != 0 && j <= (int)sqrt(m) && m % j != 0; j++)//sqrt查询是double类型，于是我直接强制类型转换成int，不影响结果
		{
			while (j >= (int)sqrt(m))
			{
				printf("%d ", m);
				count++;
				break;
			}
		}
	}
	printf("\n%d\n", count);

	return 0;
}
